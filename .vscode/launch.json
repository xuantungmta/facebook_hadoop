  "version": "0.2.0",
  "configurations": [
    {
      "name": "Java",
      "type": "java",
      "request": "launch",
      "stopOnEntry": true,
      "jdkPath": "${env:JAVA_HOME}/bin",
      "cwd": "${fileDirname}",
      "startupClass": "${fileBasenameNoExtension}",
      "classpath": [".", "${fileDirname}"]
    },
    {
      "name": "Java Console App",
      "type": "java",
      "request": "launch",
      "stopOnEntry": true,
      "jdkPath": "${env:JAVA_HOME}/bin",
      "cwd": "${fileDirname}",
      "startupClass": "${fileBasenameNoExtension}",
      "classpath": [".", "${fileDirname}"],
      "externalConsole": true
    },
    {
      "type": "java",
      "name": "Current File",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "type": "java",
      "name": "App",
      "request": "launch",
      "mainClass": "facebook_hadoop.App",
      "projectName": "app",
      "args": [
        "/home/m0lw9re/Downloads/filtered_output_utf8.csv",
        "/home/m0lw9re/Downloads/out_0001.txt"
      ]
    },
    {
      "type": "java",
      "name": "NerRecognizer",
      "request": "launch",
      "mainClass": "facebook_hadoop.corenlp.ner.NerRecognizer",
      "projectName": "app"
    },
    {
      "type": "java",
      "name": "DependencyParser",
      "request": "launch",
      "mainClass": "facebook_hadoop.corenlp.parser.DependencyParser",
      "projectName": "app"
    },
    {
      "type": "java",
      "name": "Regex",
      "request": "launch",
      "mainClass": "facebook_hadoop.corenlp.tokenizer.Regex",
      "projectName": "app"
    },
    {
      "type": "java",
      "name": "VnCoreNLP",
      "request": "launch",
      "mainClass": "facebook_hadoop.pipeline.VnCoreNLP",
      "projectName": "app"
    }
  ]
}
